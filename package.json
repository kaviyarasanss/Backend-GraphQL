{
  "name": "graphql-task",
  "version": "1.0.0",
  "description": "Backend test task using GraphQL",
  "main": "dist/main.js",
  "scripts": {
    "start:dev": "nest start --watch",
    "start": "node dist/main.js",
    "build": "nest build",
    "migrate:make": "knex --knexfile src/db/knexfile.ts migrate:make",
    "migrate:up:single": "npx knex --knexfile src/db/knexfile.ts migrate:up --specific",
    "migrate:down:single": "npx knex --knexfile src/db/knexfile.ts migrate:down --specific",
    "migration:latest": "knex --knexfile src/db/knexfile.ts migrate:latest",
    "migration:rollback": "knex --knexfile src/db/knexfile.ts migrate:rollback",
    "seed:make": "knex --knexfile src/db/knexfile.ts seed:make",
    "seed:run": "knex --knexfile src/db/knexfile.ts seed:run",
    "test": "jest --runInBand",
    "test:watch": "jest --watch"
  },
  "author": "Kaviyarasan S",
  "license": "ISC",
  "dependencies": {
    "@as-integrations/express5": "^1.1.2",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/common": "^11.1.7",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.7",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/platform-express": "^11.1.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "graphql": "^16.11.0",
    "knex": "^3.1.0",
    "objection": "^3.1.5",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
